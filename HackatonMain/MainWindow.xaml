<Window x:Class="HackatonMain.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HackatonMain"
        mc:Ignorable="d"
        ResizeMode="CanMinimize"
        SizeToContent="WidthAndHeight"
        Title="Why.exe" MaxHeight="450" MaxWidth="800" MouseLeftButtonUp="Window_MouseLeftButtonUp" Closing="Window_Closing">
    <Window.Content>
        <StackPanel Orientation="Vertical">

            <Frame Navigating="_main_Navigating" NavigationUIVisibility="Hidden" Name="_main" MouseLeftButtonUp="Window_MouseLeftButtonUp"/>
            <TextBlock Background="Aqua" Name="HelpText">
                Need help ? <Hyperlink Click="Hyperlink_Click">Click here</Hyperlink>.
            </TextBlock>
        </StackPanel>
    </Window.Content>

</Window>
    
